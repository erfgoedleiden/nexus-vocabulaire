      dash:editor memorix:DateEditor ;
    ] ;
  sh:property [
      rdfs:label "Datering betreft/opmerking"@nl ;
      sh:path object:datingComments ;
      sh:maxCount 1 ;
      sh:order 0.5 ;
      sh:datatype xsd:string ;
      dash:editor  dash:TextAreaEditor ;
    ] ;
.

object:ZoningPlan
  a sh:NodeShape ;
