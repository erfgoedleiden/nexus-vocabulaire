@prefix complex:      </resources/recordtypes/Complex#> .
@prefix dash:         <http://datashapes.org/dash#> .
@prefix dc:           <http://purl.org/dc/terms/> .
@prefix dce:          <http://purl.org/dc/elements/1.1/> .
@prefix geo:          <http://www.opengis.net/ont/geosparql#> .
@prefix html:         <http://www.w3.org/1999/xhtml/> .
@prefix memorix:      <http://memorix.io/ontology#> .
@prefix rdf:          <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:         <http://www.w3.org/2000/01/rdf-schema#> .
@prefix recordtypes:  </resources/recordtypes/> .
@prefix rico:         <https://www.ica.org/standards/RiC/ontology#> .
@prefix sh:           <http://www.w3.org/ns/shacl#> .
@prefix skos:         <http://www.w3.org/2004/02/skos/core#> .
@prefix vocabularies: </resources/vocabularies/> .
@prefix xsd:          <http://www.w3.org/2001/XMLSchema#> .

recordtypes:Complex
  a memorix:Recordtype, sh:NodeShape ;
  rdfs:label "Complex"@nl, "Complex"@en ;
  rdfs:comment "Erfgoedregister - Complexen van Erfgoed Leiden"@nl, "Heritage register Complex - Erfgoed Leiden"@en  ;
  dce:identifier "Complex" ;
  sh:closed true ;
  sh:ignoredProperties ( rdf:type ) ;
  sh:targetClass recordtypes:Complex ;
  dash:defaultViewForRole dash:all ;
  sh:property [
    rdfs:label        "Registratienummer"@nl, "Registration number"@en ;
    rdfs:comment "Identificatienummer van het Complex. Een Complex is een overkoepelende registratie van onderdelen die individueel als beschermd monument zijn aangewezen. Het dient om de samenhang en de context van die onderdelen te duiden. Complexen kennen we alleen bij Rijksmonumenten en Gemeentelijk monumenten."@nl ;   
    sh:path           rico:identifier ;
    memorix:inTitleAt 1.0 ;
    sh:maxCount       1 ;
    sh:order          1.0 ;
    sh:group          complex:complexGroup ;
    sh:datatype       xsd:string ;
    dash:singleLine   true ;
  ] ;
  sh:property [
    rdfs:label        "Complexnaam"@nl, "Name of the complex"@en ;
    memorix:inTitleAt 2.0 ;
    sh:path           rico:title ;
    sh:maxCount       1 ;
    sh:order          2.0 ;
    sh:group          complex:complexGroup ;
    sh:datatype       xsd:string ;
    dash:singleLine   true ;
  ] ;
  sh:property [
    rdfs:label  "Omschrijving"@nl, "Description"@en ;
    rdfs:comment "Omschrijving van het complex, zoals deze is vastgesteld bij de aanwijzing"@nl ;   
    sh:path     dc:description ;
    sh:maxCount 1 ;
    sh:order    3.0 ;
    sh:group    complex:complexGroup ;
    sh:datatype xsd:string ;
    dash:editor dash:TextAreaEditor ;
  ] ;
  sh:property [
    rdfs:label      "Geometrie"@nl, "Geometry"@en ;
    sh:path         geo:asWKT ;
    sh:maxCount     1 ;
    sh:order        4.0 ;
    sh:group        complex:complexGroup ;
    sh:datatype     geo:wktLiteral ;
    dash:singleLine true ;
  ] ;
.
complex:complexGroup a sh:PropertyGroup ;
  rdfs:label "Complex"@nl, "Complex"@en ;
  sh:order 1.0
.
