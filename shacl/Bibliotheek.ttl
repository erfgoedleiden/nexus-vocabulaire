@prefix bibliotheek:  </resources/recordtypes/Bibliotheek#> .
@prefix dash:         <http://datashapes.org/dash#> .
@prefix dbpo:         <http://dbpedia.org/ontology/> .
@prefix dc:           <http://purl.org/dc/terms/> .
@prefix dce:          <http://purl.org/dc/elements/1.1/> .
@prefix elo:          <http://erfgoedleiden.github.io/nexus-vocabulaire/def/vocab#> .
@prefix gn:           <http://www.geonames.org/ontology#> .
@prefix html:         <http://www.w3.org/1999/xhtml/> .
@prefix memorix:      <http://memorix.io/ontology#> .
@prefix rdf:          <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:         <http://www.w3.org/2000/01/rdf-schema#> .
@prefix recordtypes:  </resources/recordtypes/> .
@prefix rico:         <https://www.ica.org/standards/RiC/ontology#> .
@prefix schema:       <http://schema.org/> .
@prefix sh:           <http://www.w3.org/ns/shacl#> .
@prefix skos:         <http://www.w3.org/2004/02/skos/core#> .
@prefix vocabularies: </resources/vocabularies/> .
@prefix xsd:          <http://www.w3.org/2001/XMLSchema#> .

recordtypes:Bibliotheek
  a memorix:Recordtype, sh:NodeShape ;
  rdfs:label "Bibliotheek"@nl, "Library"@en ;
  rdfs:comment "Bibliotheek collectie voor Erfgoed Leiden"@nl, "Library - Erfgoed Leiden"@en ;
  dce:identifier "Bibliotheek" ;
  sh:closed true ;
  sh:ignoredProperties ( rdf:type ) ;
  sh:targetClass recordtypes:Bibliotheek ;
  memorix:typeOf        memorix:File ;
  memorix:hasInformationComponent [ rdf:type  memorix:DepotLocationComponent, memorix:DigitalAssetComponent ] ;
  dash:defaultViewForRole dash:all ;

  sh:property           [
    rdfs:label      "Archiefbibliotheek" ;
    rdfs:comment    "Kies hier altijd 0000, Standaard bibliotheek archief. Nodig om te zorgen dat bibliotheekitems gereserveerd kunnen worden."@nl ;
    dash:editor     memorix:LinkedRecordEditor ;
    memorix:typeOf  memorix:FondsFieldLink ;
    sh:group        bibliotheek:identityGroup ;
    sh:maxCount     1 ;
    sh:minCount     1 ;
    sh:nodeKind     sh:IRI ;
    sh:or           ( [ sh:class  recordtypes:Archiefbibliotheek ] ) ;
    sh:order        0.3 ;
    # "Indicates a page or other link involved in archival of a CreativeWork"
    sh:path         rico:isRelatedTo
  ] ;
  sh:property [
    rdfs:label "Signatuur"@nl, "Identification number"@en ;
    rdfs:comment "Het nummer dat een boek of tijdschift heeft"@nl ;
    sh:path rico:identifier ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:order 1.0 ;
    memorix:inSummaryAt 1.0 ;
    sh:group bibliotheek:identityGroup ;
    memorix:typeOf     memorix:Identifier ;
    sh:datatype xsd:string ;
    dash:singleLine true ;
  ] ;
  sh:property [
    rdfs:label "Standplaats"@nl, "Depot location number"@en ;
    rdfs:comment "Alleen invullen als de standplaats afwijkt. Als dit veld leeg is, geeft de Signatuur aan waar het item in het depot te vinden is. Bij kranten altijd invullen"@nl ;           
    sh:path  rico:hasOrHadPhysicalLocation ;
    sh:maxCount 1 ;
    sh:order 2.0 ;
    sh:group bibliotheek:identityGroup ;
    sh:datatype xsd:string ;
  ] ;
  sh:property [
    rdfs:label "Documenttype"@nl, "Document type"@en ;
    rdfs:comment "Wat voor type object is het? Bijvoorbeeld: boek, tijdschrift, bidprentje, etc. Er kunnen meerdere termen worden gekozen"@nl ;
    sh:path rico:hasContentOfType ;
    sh:maxCount 10 ;
    sh:order 3.0 ;
    sh:group bibliotheek:physicalDescriptionGroup ;
    dash:editor memorix:VocabularyEditor ;
    sh:class skos:Concept ;
    memorix:conceptSchemeIn ( </resources/vocabularies/conceptschemes/5f449ae6-01a2-44a3-a5a6-f74dfa587a87> ) ;
    sh:nodeKind sh:IRI ;
  ] ;
  sh:property [
    rdfs:label "Titel werkveld"@nl ;
    rdfs:comment "Dit veld niet invullen. Hier staan oude beschrijvingen, die door de bibliothecaris worden aangepast"@nl ;
    sh:path dc:subject ;
    sh:maxCount 1 ;
    sh:order 4.0 ;
    sh:group bibliotheek:physicalDescriptionGroup ;
    sh:datatype xsd:string ;
    dash:editor  dash:TextAreaEditor ;
  ] ;
  sh:property [
    rdfs:label "Titel"@nl, "Title"@en ;
    rdfs:comment "Neem de titel over van de titelpagina (FOBID)"@nl ;
    sh:path rico:title ;
    sh:maxCount 1 ;
    sh:order 5.0 ;
    memorix:inTitleAt 1.0 ;
    sh:group bibliotheek:physicalDescriptionGroup ;
    sh:datatype xsd:string ;
    dash:editor  dash:TextAreaEditor ;
  ] ;
  sh:property [
    rdfs:label "Digitale publicatie"@nl, "Digital publication"@en ;
    rdfs:comment "Link naar een extern digitale versie/voortzetting"@nl ;
    sh:path rico:hasDerivedInstantiation ;
    sh:maxCount 10 ;
    sh:order 6.0 ;
    sh:group bibliotheek:physicalDescriptionGroup ;
    sh:datatype xsd:string ;
    dash:singleLine true ;
  ] ;
  sh:property [
    rdfs:label "Auteur"@nl, "Author"@en ;
    sh:maxCount 10 ;
    sh:path schema:author ;
    sh:order 7.0 ;
    dash:viewer dash:DetailsEditor ;
    sh:nodeKind sh:BlankNode ;
    sh:class bibliotheek:Author ;
    sh:group bibliotheek:physicalDescriptionGroup ;
  ] ;
  sh:property [
    rdfs:label "Editie"@nl, "Edition"@en ;
    rdfs:comment "Bijvoorbeeld: 2e dr./geh. herz. ed. (FOBID)"@nl;  
    sh:path schema:bookEdition ;
    sh:maxCount 1 ;
    sh:order 9.5 ;
    sh:group bibliotheek:physicalDescriptionGroup ;
    sh:datatype xsd:string ;
    dash:singleLine true ;
  ] ;
  sh:property [
    rdfs:label "Plaats van uitgave"@nl, "Publication place"@en ;
    sh:maxCount 10 ;
    sh:path schema:locationCreated ;
    sh:order 8.0 ;
    dash:viewer dash:DetailsEditor ;
    sh:nodeKind sh:BlankNode ;
    sh:class bibliotheek:PlaceOfPublication ;
    sh:group bibliotheek:physicalDescriptionGroup ;
  ] ;
  sh:property [
    rdfs:label "Uitgever"@nl, "Publisher"@en ;
    sh:path schema:publisher ;
    sh:order 9.0 ;
    sh:maxCount 5 ;
    sh:group bibliotheek:physicalDescriptionGroup ;
    sh:nodeKind sh:IRI ;
    sh:or (
      [ sh:class recordtypes:Persoon ]
      [ sh:class recordtypes:Organisatie ]
    ) ;
    dash:editor memorix:LinkedRecordEditor ;
  ] ;
  sh:property [
    rdfs:label "Datum van uitgave"@nl, "Publication date"@en ;
    sh:maxCount 10 ;
    sh:path schema:datePublished ;
    sh:order 10.0 ;
    dash:viewer dash:DetailsEditor ;
    sh:nodeKind sh:BlankNode ;
    sh:class bibliotheek:PublicationDate ;
    sh:group bibliotheek:physicalDescriptionGroup ;
  ] ;
  sh:property [
    rdfs:label "Pagina's"@nl, "Number of pages"@en ;
    sh:path schema:numberOfPages ;
    sh:maxCount 1 ;
    sh:order 11.0 ;
    sh:group bibliotheek:physicalDescriptionGroup ;
    sh:datatype xsd:string ;
    dash:singleLine true ;
  ] ;
  sh:property [
    rdfs:label "Geïllustreerd"@nl, "Illustrated"@en ;
    sh:path schema:image ;
    sh:maxCount 1 ;
    sh:order 12.0 ;
    sh:group bibliotheek:physicalDescriptionGroup ;
    sh:datatype xsd:string ;
    dash:singleLine true ;
  ] ;
  sh:property [
    rdfs:label "Serie"@nl, "Series"@en ;
    sh:path schema:partOfSeries ;
    sh:maxCount 1 ;
    sh:order 13.0 ;
    sh:group bibliotheek:physicalDescriptionGroup ;
    dash:editor memorix:VocabularyEditor ;
    sh:class skos:Concept ;
    memorix:conceptSchemeIn ( </resources/vocabularies/conceptschemes/5ca204af-c3c4-42ad-9d18-af483670d8f5> ) ;
    sh:nodeKind sh:IRI ;
  ] ;
  sh:property [
    rdfs:label "Annotatie"@nl, "Annotation"@en ;
    sh:path dbpo:hasAnnotation ;
    sh:maxCount 1 ;
    sh:order 14.0 ;
    sh:group bibliotheek:physicalDescriptionGroup ;
    sh:datatype xsd:string ;
    dash:editor  dash:TextAreaEditor ;
  ] ;
  sh:property [
    rdfs:label "ISBN"@nl, "ISBN"@en ;
    sh:path schema:isbn ;
    sh:maxCount 1 ;
    sh:order 15.0 ;
    sh:group bibliotheek:physicalDescriptionGroup ;
    sh:datatype xsd:string ;
    dash:singleLine true ;
  ] ;

  sh:property [
    rdfs:label "Korte inhoud"@nl, "Content"@en ;
    sh:path schema:abstract ;
    sh:maxCount 1 ;
    sh:order 16.0 ;
    sh:group bibliotheek:contentDescriptionGroup ;
    sh:datatype xsd:string ;
    dash:editor  dash:TextAreaEditor ;
  ] ;
  sh:property [
    rdfs:label "Persoon"@nl, "Person"@en ;
    sh:path schema:character ;
    sh:maxCount 100 ;
    sh:order 17.0 ;
    sh:group bibliotheek:contentDescriptionGroup ;
    sh:nodeKind sh:IRI ;
    sh:or (
      [ sh:class recordtypes:Persoon ]
      [ sh:class recordtypes:Organisatie ]
    ) ;
    dash:editor memorix:LinkedRecordEditor ;
  ] ;
  sh:property [
    rdfs:label "Onderwerp"@nl, "Subjects"@en ;
    # "The subject matter of the content."
    sh:path schema:about ;
    sh:maxCount 20 ;
    sh:order 18.0 ;
    sh:group bibliotheek:contentDescriptionGroup ;
    dash:editor memorix:VocabularyEditor ;
    sh:class skos:Concept ;
    memorix:conceptSchemeIn ( </resources/vocabularies/conceptschemes/4190de09-7dca-44ce-a9bd-04716cfa70aa> ) ;
    sh:nodeKind sh:IRI ;
  ] ;
  sh:property [
    rdfs:label "Opmerking inhoudelijk"@nl, "Remarks"@en ;
    sh:path rico:descriptiveNote ;
    sh:maxCount 1 ;
    sh:order 19.0 ;
    sh:group bibliotheek:contentDescriptionGroup ;
    sh:datatype xsd:string ;
    dash:editor  dash:TextAreaEditor ;
  ] ;
  sh:property [
    rdfs:label "Locatie"@nl, "Location"@en ;
    sh:maxCount 10 ;
    # "The location depicted or described in the content. For example, the location in a photograph or painting. "
    sh:path schema:contentLocation ;
    sh:order 20.0 ;
    dash:viewer dash:DetailsEditor ;
    sh:nodeKind sh:BlankNode ;
    sh:class bibliotheek:Location ;
    sh:group bibliotheek:contentDescriptionGroup ;
  ] ;

  sh:property [
    rdfs:label "Bron"@nl, "Source"@en ;
    sh:maxCount 10 ;
    sh:path dc:source ;
    sh:order 21.0 ;
    dash:viewer dash:DetailsEditor ;
    sh:nodeKind sh:BlankNode ;
    sh:class bibliotheek:Source ;
    sh:group bibliotheek:partOfGroup ;
  ] ;
  sh:property [
    rdfs:label "Jaargang"@nl, "Volume"@en ;
    sh:path schema:volumeNumber ;
    sh:maxCount 1 ;
    sh:order 22.0 ;
    sh:group bibliotheek:partOfGroup ;
    sh:datatype xsd:string ;
    dash:singleLine true ;
  ] ;
  sh:property [
    rdfs:label "Aflevering"@nl, "Episode"@en ;
    sh:path schema:episode ;
    sh:maxCount 1 ;
    sh:order 23.0 ;
    sh:group bibliotheek:partOfGroup ;
    sh:datatype xsd:string ;
    dash:singleLine true ;
  ] ;
  sh:property [
    rdfs:label "Deel"@nl, "Part"@en ;
    sh:path schema:hasPart ;
    sh:maxCount 1 ;
    sh:order 24.0 ;
    sh:group bibliotheek:partOfGroup ;
    sh:datatype xsd:string ;
    dash:singleLine true ;
  ] ;
  sh:property [
    rdfs:label "Nummering"@nl, "Number"@en ;
    sh:path schema:position ;
    sh:maxCount 1 ;
    sh:order 25.0 ;
    sh:group bibliotheek:partOfGroup ;
    sh:datatype xsd:string ;
    dash:singleLine true ;
  ] ;
  sh:property [
    rdfs:label "Aanwezig"@nl, "Present"@en ;
    sh:path schema:availability ;
    sh:maxCount 1 ;
    sh:order 26.0 ;
    sh:group bibliotheek:partOfGroup ;
    sh:datatype xsd:string ;
    dash:singleLine true ;
  ] ;
  sh:property [
    rdfs:label "Ingebonden in"@nl, "Bound in"@en ;
    sh:path schema:isPartOf ;
    sh:maxCount 10 ;
    sh:order 27.0 ;
    sh:group bibliotheek:partOfGroup ;
    sh:datatype xsd:string ;
    dash:singleLine true ;
  ] ;

  sh:property [
    rdfs:comment  "Bijzonderheid bij dit object"@nl ;
    rdfs:label    "Bijzonderheid"@nl, "Details"@en ;
    dash:viewer   dash:DetailsEditor ;
    sh:class      bibliotheek:Details ;
    sh:group      bibliotheek:detailsGroup ;
    sh:maxCount   10 ;
    sh:nodeKind   sh:BlankNode ;
    sh:order      8.0 ;
    sh:path       schema:comment
  ] ;

  sh:property [
    rdfs:label "Recordtype"@nl ;
    sh:path rico:hasRecordSetType ;
    sh:maxCount 1 ;
    sh:order 28.0 ;
    sh:group bibliotheek:managementGroup ;
    dash:editor memorix:VocabularyEditor ;
    sh:class skos:Concept ;
    memorix:conceptSchemeIn ( </resources/vocabularies/conceptschemes/28a07f1f-a398-4d81-b907-a0a356f3429b> ) ;
    sh:nodeKind sh:IRI ;
  ] ;
  sh:property [
    rdfs:label "Opmerking beheerder"@nl, "Internal remarks"@en ;
    sh:path elo:opmerkingBeheerder ;
    sh:maxCount 1 ;
    sh:order 29.0 ;
    sh:group bibliotheek:managementGroup ;
    sh:datatype xsd:string ;
    dash:editor  dash:TextAreaEditor ;
  ] ;
  sh:property [
    rdfs:label "Retro"@nl, "Retro"@en ;
    # Bestaat wegens vroegere documentatiemethode
    sh:path elo:retro ;
    sh:maxCount 1 ;
    sh:order 30.0 ;
    sh:group bibliotheek:managementGroup ;
    sh:datatype xsd:string ;
    dash:editor  dash:TextAreaEditor ;
  ] ;
  sh:property [
    rdfs:label "Aanwinst"@nl, "Acquisition"@en ;
    sh:path dbpo:startYearOfInsertion ;
    sh:maxCount 1 ;
    sh:order 31.0 ;
    sh:group bibliotheek:managementGroup ;
    sh:datatype xsd:gYear ;
  ] ;
  sh:property [
    rdfs:label "Archief"@nl, "Fonds"@en ;
    sh:path schema:archivedAt ;
    sh:maxCount 1 ;
    sh:order 32.0 ;
    sh:group bibliotheek:managementGroup ;
    sh:datatype xsd:boolean ;
  ] ;
  sh:property [
    rdfs:label "Gecontroleerd"@nl, "Checked"@en ;
    sh:path dc:valid ;
    sh:maxCount 1 ;
    sh:order 33.0 ;
    sh:group bibliotheek:managementGroup ;
    sh:datatype xsd:boolean ;
  ] ;
  sh:property [
    rdfs:label "Op studiezaal"@nl, "In study room"@en ;
    sh:path schema:publicAccess ;
    sh:maxCount 1 ;
    sh:order 34.0 ;
    sh:group bibliotheek:managementGroup ;
    sh:datatype xsd:boolean ;
  ] ;
  sh:property [
    rdfs:label "Verwerving"@nl, "Aquisition"@en ;
    sh:maxCount 1 ;
    # "The organization or person from which the product was acquired."
    sh:path schema:acquiredFrom ;
    sh:order 34.0 ;
    dash:viewer dash:DetailsEditor ;
    sh:nodeKind sh:BlankNode ;
    sh:class bibliotheek:Acquisition ;
    sh:group bibliotheek:managementGroup ;
  ] ;
.

bibliotheek:Author
  a sh:NodeShape ;
  sh:closed true ;
  sh:ignoredProperties ( rdf:type ) ;
  sh:targetClass bibliotheek:Author ;
  sh:property [
    rdfs:label "Auteur als in publicatie"@nl, "Author as stated on the publication"@en ;
    sh:path schema:author ;
    sh:maxCount 1 ;
    sh:order 1.0 ;
    memorix:inSummaryAt 2.0 ;
    dash:editor memorix:VocabularyEditor ;
    sh:class skos:Concept ;
    memorix:conceptSchemeIn ( </resources/vocabularies/conceptschemes/304b0173-711b-4670-8045-5d85cb8b24b1> ) ;
    sh:nodeKind sh:IRI ;
  ] ;

  sh:property [
    rdfs:label "Auteur / maker"@nl, "Author/creator"@en ;
    sh:path dc:creator ;
    sh:order 2.0 ;
    sh:nodeKind sh:IRI ;
    sh:or (
      [ sh:class recordtypes:Persoon ]
      [ sh:class recordtypes:Organisatie ]
    ) ;
    dash:editor memorix:LinkedRecordEditor ;
  ] ;
  sh:property [
    rdfs:label "Rol"@nl, "Role"@en ;
    sh:path schema:roleName ;
    sh:maxCount 5 ;
    sh:order 3.0 ;
    dash:editor memorix:VocabularyEditor ;
    sh:class skos:Concept ;
    memorix:conceptSchemeIn ( </resources/vocabularies/conceptschemes/efcca629-b7ad-4bb0-b9f5-ab88b57cb79c> ) ;
    sh:nodeKind sh:IRI ;
  ] ;
.

bibliotheek:PlaceOfPublication
  a sh:NodeShape ;
  sh:closed true ;
  sh:ignoredProperties ( rdf:type ) ;
  sh:targetClass bibliotheek:PlaceOfPublication ;
  sh:property [
    rdfs:label "Plaats van uitgave geonames"@nl ;
    # In GeoNames: "A feature code."
    sh:path gn:Code  ;
    sh:order 2.0 ;
    sh:datatype gn:geonamesID ;
    dash:editor dash:TextFieldEditor ;
    memorix:searchWeight 1.0 ;
    # TODO dit moet een link naar Geonames worden
  ] ;
  sh:property [
    rdfs:label "Plaats van uitgave als in publicatie"@nl, "Publication place as stated on the publication"@en ;
    sh:path schema:addressLocality ;
    sh:maxCount 1 ;
    sh:order 1.0 ;
    sh:datatype xsd:string ;
    dash:singleLine true ;
  ] ;
.

bibliotheek:PublicationDate
  a sh:NodeShape ;
  sh:closed true ;
  sh:ignoredProperties ( rdf:type ) ;
  sh:targetClass bibliotheek:PublicationDate ;
  sh:property [
    rdfs:label "Jaar van uitgave"@nl, "Year of publication"@en ;
    sh:path schema:datePublished ;
    sh:maxCount 1 ;
    sh:order 1.0 ;
    memorix:inSummaryAt 3.0 ;
    sh:datatype xsd:string ;
    dash:singleLine true ;
  ] ;
  sh:property [
    rdfs:label "Jaar van uitgave / datum publicatie (van)"@nl, "Year of publication (from)"@en ;
    sh:path schema:startDate ;
    sh:order 2.0 ;
    sh:datatype xsd:integer ;
    dash:editor memorix:DateEditor ;
  ] ;
  sh:property [
    rdfs:label "Jaar van uitgave / datum publicatie (tot)"@nl, "Year of publication (until)"@en ;
    sh:path schema:endDate ;
    sh:order 3.0 ;
    sh:datatype xsd:integer ;
    dash:editor memorix:DateEditor ;
  ] ;
.

bibliotheek:Location
  a sh:NodeShape ;
  sh:closed true ;
  sh:ignoredProperties ( rdf:type ) ;
  sh:targetClass bibliotheek:Location ;

  sh:property [
    rdfs:label "Regio trefwoord"@nl, "Location"@en ;
    sh:path schema:addressRegion ;
    sh:maxCount 100 ;
    sh:order 1.0 ;
    dash:editor memorix:VocabularyEditor ;
    sh:class skos:Concept ;
    memorix:conceptSchemeIn (
        </resources/vocabularies/conceptschemes/026baf4a-c14f-4819-be3e-2301fb6b4a67>
        </resources/vocabularies/conceptschemes/2540c439-bebf-4ce7-bca9-3c3cba71a6c9>
        </resources/vocabularies/conceptschemes/5e2c373d-3451-3ce6-af6a-cdbc6fba861e> ) ;
    sh:nodeKind sh:IRI ;
  ] ;
  sh:property [
      rdfs:label "Werkveld locatie"@nl, "Workfield location"@en ;
      sh:path schema:location ;
      sh:order 2.0 ;
      sh:datatype xsd:string ;
  ] ;
  sh:property [
      rdfs:label "Coördinaten"@nl, "Coordinates"@en ;
      sh:path schema:geo ;
      sh:maxCount 10 ;
      sh:order 3.0 ;
      sh:datatype xsd:string ;
      memorix:searchWeight 1.0 ;
    # TODO dit moet een link naar coordinates
  ] ;
.

bibliotheek:Source
  a sh:NodeShape ;
  sh:closed true ;
  sh:ignoredProperties ( rdf:type ) ;
  sh:targetClass bibliotheek:Source ;
  sh:property [
    rdfs:label "Bron tekst"@nl, "Source text"@en ;
    sh:path dc:source ;
    sh:maxCount 1 ;
    sh:order 1.0 ;
    sh:datatype xsd:string ;
    dash:singleLine true ;
  ] ;
  sh:property [
    rdfs:label "Bron link"@nl, "Source link"@en ;
    sh:path rico:source ;
    sh:maxCount 5 ;
    sh:order 2.0 ;
    sh:nodeKind sh:IRI ;
    sh:or (
      [ sh:class recordtypes:Bibliotheek ]
    ) ;
    dash:editor memorix:LinkedRecordEditor ;
  ] ;
.

bibliotheek:Acquisition
  a sh:NodeShape ;
  sh:closed true ;
  sh:ignoredProperties ( rdf:type ) ;
  sh:targetClass bibliotheek:Acquisition ;
  sh:property [
    rdfs:label "Herkomst"@nl ;
    # "Summary of the development of an entity, since its origin until present time."
    sh:path rico:history ;
    sh:maxCount 1 ;
    sh:order 1.0 ;
    dash:editor memorix:VocabularyEditor ;
    sh:class skos:Concept ;
    memorix:conceptSchemeIn ( </resources/vocabularies/conceptschemes/0afc54d2-d623-4ab3-9e27-b1ea6d866cc1> ) ;
    sh:nodeKind sh:IRI ;
  ] ;
  sh:property [
    rdfs:label "Persoon/Organisatie"@nl ;
    # "Connects a Record Resource or Relation to a Record Resource or Agent that is used as a source of information for
    #  identifying or describing it."
    sh:path rico:hasSource ;
    sh:maxCount 10 ;
    sh:order 2.0 ;
    sh:nodeKind sh:IRI ;
    sh:or (
      [ sh:class recordtypes:Persoon ]
      [ sh:class recordtypes:Organisatie ]
    ) ;
    dash:editor memorix:LinkedRecordEditor ;
  ] ;
  sh:property [
    rdfs:label "Verwervingsdatum"@nl ;
    sh:path schema:ownedFrom ;
    sh:maxCount 1 ;
    sh:order 3.0 ;
    sh:datatype xsd:integer ;
    dash:editor memorix:DateEditor ;
  ] ;
  sh:property [
    rdfs:label "Opmerking verwerving"@nl ;
    sh:path rico:descriptiveNote ;
    sh:maxCount 1 ;
    sh:order 4.0 ;
    sh:datatype xsd:string ;
    dash:editor  dash:TextAreaEditor ;
  ] ;
.

bibliotheek:Details
  a sh:NodeShape ;
  sh:closed true ;
  sh:ignoredProperties ( rdf:type ) ;
  sh:targetClass bibliotheek:Details ;
  sh:property [
    rdfs:label   "Bijzonderheid - Type"@nl ;
    sh:order     1.0 ;
    # Niet duidelijk of dit de juiste property is
    sh:path      rico:hasContentOfType ;
    dash:editor memorix:VocabularyEditor ;
    sh:class skos:Concept ;
    memorix:conceptSchemeIn ( </resources/vocabularies/conceptschemes/e2f783dd-710d-473a-9515-b97eaa0da52d> ) ;
    sh:nodeKind sh:IRI ;
  ] ;
  sh:property [
    rdfs:label   "Bijzonderheid"@nl ;
    sh:datatype  xsd:string ;
    sh:order     2.0 ;
    sh:path      rico:descriptiveNote
  ] ;
.

bibliotheek:identityGroup a sh:PropertyGroup ;
  rdfs:label "Vindplaats"@nl ;
  sh:order 1.0
