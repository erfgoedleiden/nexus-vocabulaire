@prefix aanwinst:     </resources/recordtypes/Aanwinst#> .
@prefix dash:         <http://datashapes.org/dash#> .
@prefix dc:           <http://purl.org/dc/terms/> .
@prefix dce:          <http://purl.org/dc/elements/1.1/> .
@prefix elo:          <http://erfgoedleiden.github.io/nexus-vocabulaire/def/vocab#> .
@prefix html:         <http://www.w3.org/1999/xhtml/> .
@prefix memorix:      <http://memorix.io/ontology#> .
@prefix rdf:          <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:         <http://www.w3.org/2000/01/rdf-schema#> .
@prefix recordtypes:  </resources/recordtypes/> .
@prefix rico:         <https://www.ica.org/standards/RiC/ontology#> .
@prefix schema:       <http://schema.org/> .
@prefix sh:           <http://www.w3.org/ns/shacl#> .
@prefix skos:         <http://www.w3.org/2004/02/skos/core#> .
@prefix vocabularies: </resources/vocabularies/> .
@prefix xsd:          <http://www.w3.org/2001/XMLSchema#> .

aanwinst:identificationGroup
    rdf:type   sh:PropertyGroup ;
    rdfs:label "Identificatie"@nl, "Identification"@en ;
    sh:order   1.0 .

aanwinst:descriptionGroup
    rdf:type   sh:PropertyGroup ;
    rdfs:label "Beschrijving"@nl, "Description"@en ;
    sh:order   2.0 .

aanwinst:acquisitionGroup
    rdf:type   sh:PropertyGroup ;
    rdfs:label "Acquisitie"@nl, "Acquisition"@en ;
    sh:order   3.0 .

aanwinst:managementGroup
    rdf:type   sh:PropertyGroup ;
    rdfs:label "Beheer"@nl, "Management"@en ;
    sh:order 4.0 .

aanwinst:notesGroup
    rdf:type   sh:PropertyGroup ;
    rdfs:label "Opmerkingen"@nl, "Remarks"@en  ;
    sh:order 5.0 .

recordtypes:Aanwinst
    a memorix:Recordtype, sh:NodeShape ;
    rdfs:label "Aanwinst"@nl, "Acquisition"@en ;
    rdfs:comment "Aanwinst - Erfgoed Leiden"@nl, "Record type for Acquisition"@en ;
    dce:identifier "Aanwinst" ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:targetClass recordtypes:Aanwinst ;
    dash:defaultViewForRole dash:all ;
    rdfs:subClassOf                 rico:RecordSet ;
    memorix:hasInformationComponent [ rdf:type  memorix:DepotLocationComponent ] ;
    sh:property [
        sh:maxCount 1 ;
        rdfs:label "Identificatiecode"@nl, "Identification code"@en ;
        rdfs:comment "Jaartal aanwinst, streepje (-), volgnummer met 3 cijfers [jjjj-vvv]. Gebruik voor een nieuw nummer het eerstvolgende niet gebruikte nummer"@nl ;
        sh:group aanwinst:identificationGroup ;
        sh:path rico:identifier ;
        memorix:inTitleAt 1.0 ;
        sh:order 1.0 ;
        sh:datatype xsd:string ;
    ] ;
    sh:property [
        sh:maxCount 1 ;
        rdfs:label "Omschrijving"@nl, "Description"@en ;
        sh:path rico:descriptiveNote ;
        sh:order 2.0 ;
        sh:group aanwinst:identificationGroup ;
        sh:datatype xsd:string ;
        dash:editor  dash:TextAreaEditor ;
    ] ;
    sh:property [
        sh:maxCount 1 ;
        rdfs:label "Datum ontvangst"@nl, "Date of reception"@en ;
        sh:group aanwinst:descriptionGroup ;
        sh:path schema:ownedFrom ;
        sh:order 3.0 ;
        # TODO: datumveld zou geen integer moeten zijn
        sh:datatype xsd:integer ;
        dash:editor memorix:DateEditor ;
    ] ;
    sh:property [
        sh:maxCount 1 ;
        rdfs:label "Soort aanwinst"@nl, "Type of aquisition"@en ;
        sh:path rico:hasContentOfType ;
        sh:order 4.0 ;
        memorix:inTitleAt 2.0 ;
        sh:group aanwinst:descriptionGroup ;
        dash:editor             dash:EnumSelectEditor ;
        sh:class skos:Concept ;
        memorix:conceptSchemeIn ( </resources/vocabularies/conceptschemes/af5e1f30-ab30-33bf-9f81-6063449ee0c8> ) ;
        sh:nodeKind sh:IRI ;
    ] ;
    sh:property [
        sh:maxCount 1 ;
        rdfs:label   "Omvang (meters)"@nl, "Extent (m)"@en ;
        sh:datatype  xsd:decimal ;
        sh:order     5.0 ;
        sh:path      rico:recordResourceExtent ;
        sh:group aanwinst:descriptionGroup ;
    ] ;
    sh:property [
        rdfs:label "Datering van archief"@nl, "Date of fonds"@en ;
        sh:maxCount 1 ;
        sh:path rico:isAssociatedWithDate ;
        sh:order 6.0 ;
        dash:viewer dash:DetailsEditor ;
        sh:nodeKind sh:BlankNode ;
        sh:class rico:DateRange ;
        sh:group aanwinst:descriptionGroup ;
    ] ;
    sh:property [
        rdfs:label "Contact"@nl, "Contact"@en ;
        sh:maxCount 1 ;
        sh:path schema:contactPoint ;
        sh:order 7.0 ;
        dash:viewer dash:DetailsEditor ;
        sh:nodeKind sh:BlankNode ;
        sh:class aanwinst:Contact ;
        sh:group aanwinst:descriptionGroup ;
    ] ;
    sh:property [
        sh:maxCount 1 ;
        rdfs:label   "Datum eerste contact"@nl, "Date of first contact"@en ;
        sh:order     8.0 ;
        sh:path      elo:datumEersteContact ;
        sh:group     aanwinst:acquisitionGroup ;
        dash:editor  memorix:DateEditor ;
        sh:datatype  xsd:integer ;
        sh:maxLength 8 ;
    ] ;
    sh:property [
        sh:maxCount 1 ;
        rdfs:label   "Overeenkomst gestuurd"@nl, "Agreement sent"@en ;
        sh:order     9.0 ;
        sh:path      elo:datumOvereenkomstGestuurd ;
        sh:group     aanwinst:acquisitionGroup ;
        dash:editor  memorix:DateEditor ;
        sh:datatype  xsd:integer ;
        sh:maxLength 8 ;
    ] ;
    sh:property [
        sh:maxCount 1 ;
        rdfs:label   "Overeenkomst retour"@nl, "Return agreement"@en ;
        sh:order     10.0 ;
        sh:path      elo:datumOvereenkomstRetour ;
        sh:group     aanwinst:acquisitionGroup ;
        dash:editor  memorix:DateEditor ;
        sh:datatype  xsd:integer ;
        sh:maxLength 8 ;
    ] ;
    sh:property [
        sh:maxCount 1 ;
        rdfs:label   "Akkoord Inspectie en Acquisitie"@nl, "Agreement Inspection and Aquisition"@en ;
        sh:order     11.0 ;
        sh:path      elo:datumAkkoordInspectieEnAcquisitie ;
        sh:group     aanwinst:acquisitionGroup ;
        dash:editor  memorix:DateEditor ;
        sh:datatype  xsd:integer ;
        sh:maxLength 8 ;
    ] ;
    sh:property [
        sh:maxCount 1 ;
        rdfs:label   "Akkoord Restauratie"@nl, "Agreement restoration"@en ;
        sh:order     12.0 ;
        sh:path      elo:datumAkkoordRestauratie ;
        sh:group     aanwinst:acquisitionGroup ;
        dash:editor  memorix:DateEditor ;
        sh:datatype  xsd:integer ;
        sh:maxLength 8 ;
    ] ;
    sh:property [
        sh:maxCount 1 ;
        rdfs:label   "(Her)verpakking"@nl, "(Re)packaging"@en ;
        sh:order     14.0 ;
        sh:path      elo:datumVerpakkingOfHerverpakking ;
        sh:group     aanwinst:acquisitionGroup ;
        dash:editor  memorix:DateEditor ;
        sh:datatype  xsd:integer ;
        sh:maxLength 8 ;
    ] ;
    sh:property [
        sh:maxCount 1 ;
        rdfs:label   "Binnenkomst depot"@nl, "Depot entry"@en ;
        sh:order     15.0 ;
        sh:path      elo:datumBinnenkomstDepot ;
        sh:group     aanwinst:acquisitionGroup ;
        dash:editor  memorix:DateEditor ;
        sh:datatype  xsd:integer ;
        sh:maxLength 8 ;
    ] ;
    sh:property [
        sh:maxCount 1 ;
        rdfs:label   "Akkoord schade inv."@nl, "Agreement damage inventory"@en ;
        sh:order     16.0 ;
        sh:path      elo:datumAkkoordSchadeInventarisatie ;
        sh:group     aanwinst:acquisitionGroup ;
        dash:editor  memorix:DateEditor ;
        sh:datatype  xsd:integer ;
        sh:maxLength 8 ;
    ] ;
    sh:property [
        sh:maxCount 1 ;
        rdfs:label   "Datum ontvangen toegang"@nl, "Date of receipt"@en ;
        sh:order     17.0 ;
        sh:path      elo:datumOntvangenToegang ;
        sh:group     aanwinst:acquisitionGroup ;
        dash:editor  memorix:DateEditor ;
        sh:datatype  xsd:integer ;
        sh:maxLength 8 ;
    ] ;
    sh:property [
        rdfs:label "Definitieve acquisitie"@nl, "Completed acquisition"@en ;
        sh:maxCount 1 ;
        sh:path      elo:definitieveAcquisitie ;
        sh:order 18.0 ;
        sh:group aanwinst:acquisitionGroup ;
        sh:datatype xsd:boolean ;
    ] ;
    sh:property [
        sh:maxCount 1 ;
        rdfs:label "Bijzonderheden inspectie"@nl, "Remarks inspection"@en ;
        sh:path elo:opmerkingBeheerder ;
        sh:order 19.0 ;
        sh:group aanwinst:acquisitionGroup ;
        sh:datatype xsd:string ;
        dash:editor  dash:TextAreaEditor ;
    ] ;
    sh:property [
        rdfs:label "Volledig openbaar"@nl, "Completely public"@en ;
        sh:maxCount 1 ;
        # Redelijk passend: "A flag to signal that the Place is open to public visitors."
        sh:path schema:publicAccess ;
        sh:order 20.0 ;
        sh:group aanwinst:managementGroup ;
        sh:datatype xsd:boolean ;
    ] ;
    sh:property [
        sh:maxCount 1 ;
        rdfs:label "Niet openbare stukken - periode"@nl, "Non-public documents - period"@en ;
        # Geen goed passende term
        sh:path elo:nietOpenbaarBereik ;
        sh:order 21.0 ;
        sh:group aanwinst:managementGroup ;
        sh:datatype xsd:string ;
        dash:editor  dash:TextAreaEditor ;
    ] ;
    sh:property [
        rdfs:label "Gedeelte voor vernietiging vatbaar"@nl ;
        sh:maxCount 1 ;
        sh:path elo:aangemerktVoorVernietiging ;
        sh:order 22.0 ;
        sh:group aanwinst:managementGroup ;
        sh:datatype xsd:boolean ;
    ] ;
    sh:property [
        rdfs:label "Beeldmateriaal"@nl, "Contains images"@en ;
        rdfs:comment "Bevat foto's, etc"@nl ;
        sh:maxCount 1 ;
        # Geen goed passende term
        sh:path schema:image ;
        sh:order 23.0 ;
        sh:group aanwinst:managementGroup ;
        sh:datatype xsd:boolean ;
    ] ;
    sh:property [
        rdfs:label "Opgegaan in"@nl, "Merged into"@en ;
        sh:maxCount 1 ;
        sh:path rico:isOrWasPartOf ;
        sh:order 24.0 ;
        dash:viewer dash:DetailsEditor ;
        sh:nodeKind sh:BlankNode ;
        sh:class aanwinst:MergedInto ;
        sh:group aanwinst:managementGroup ;
    ] ;
    sh:property [
        rdfs:label "Aanwinst verwerkt"@nl, "Acquisition processed"@en ;
        rdfs:comment "Als alle gegevens bij het Archiefrecord zijn verwerkt, kies je hier Ja"@nl ; 
        sh:maxCount 1 ;
        sh:path elo:isVerwerkt  ;
        sh:order 25.0 ;
        sh:group aanwinst:managementGroup ;
        sh:datatype xsd:boolean ;
    ] ;
    sh:property [
        rdfs:label "Interne opmerking"@nl, "Internal remarks"@en ;
        sh:maxCount 1 ;
        sh:path elo:opmerkingIntern ;
        sh:order 26.0 ;
        sh:group aanwinst:notesGroup ;
        sh:datatype xsd:string ;
        dash:editor  dash:TextAreaEditor ;
    ] ;
.

rico:DateRange
    rdf:type             sh:NodeShape ;
    sh:closed            true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [
        rdfs:label   "Periode"@nl, "Dates of accumulation"@en ;
        rdfs:comment "Periode van de aanwinst in tekstvorm. Voorbeeld: 18e eeuw"@nl ;
        sh:maxCount 1 ;
        sh:datatype  xsd:string ;
        sh:order     1.0 ;
        sh:path      rico:expressedDate ;
    ] ;
    sh:property [
        rdfs:label   "Periode (van)"@nl, "Dates of accumulation (begin)"@en ;
        sh:maxCount 1 ;
        sh:datatype  xsd:gYear ;
        sh:order     2.0 ;
        sh:path      rico:hasBeginningDate ;
    ] ;
    sh:property [
        rdfs:label   "Periode (tot)"@nl, "Dates of accumulation (end)"@en ;
        sh:maxCount 1 ;
        sh:datatype  xsd:gYear ;
        sh:order     3.0 ;
        sh:path      rico:hasEndDate ;
    ] ;
    sh:targetClass       rico:DateRange ;
.

aanwinst:Contact
    rdf:type             sh:NodeShape ;
    sh:closed            true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:targetClass       aanwinst:Contact ;
    sh:property [
        rdfs:label   "Archiefvormer"@nl, "Accumulator"@en ;
        sh:order     1.0 ;
        sh:maxCount 100 ;
        sh:path      rico:hasAccumulator ;
        sh:nodeKind sh:IRI ;
        sh:or (
            [ sh:class recordtypes:ActorFamilie ]
            [ sh:class recordtypes:ActorOrganisatie ]
            [ sh:class recordtypes:ActorPersoon ]
        ) ;
        dash:editor memorix:LinkedRecordEditor ;
    ] ;
    sh:property [
        rdfs:label   "Eigenaar"@nl, "Owner"@en ;
        sh:order     2.0 ;
        sh:maxCount 1 ;
        sh:path      rico:hasOrHadOwner ;
        sh:nodeKind sh:IRI ;
        sh:or (
            [ sh:class recordtypes:ActorFamilie ]
            [ sh:class recordtypes:ActorOrganisatie ]
            [ sh:class recordtypes:ActorPersoon ]
        ) ;
        dash:editor memorix:LinkedRecordEditor ;
    ] ;
    sh:property [
        rdfs:label   "Contactpersoon"@nl, "Contact"@en ;
        rdfs:comment "Is meestal de schenker of bruikleengever"@nl ;
        sh:order     3.0 ;
        sh:maxCount 100 ;
        sh:path      schema:contactPoint ;
        sh:nodeKind sh:IRI ;
        sh:or (
            [ sh:class recordtypes:ActorFamilie ]
            [ sh:class recordtypes:ActorOrganisatie ]
            [ sh:class recordtypes:ActorPersoon ]
        ) ;
        dash:editor memorix:LinkedRecordEditor ;
    ] ;
.
aanwinst:MergedInto
    rdf:type             sh:NodeShape ;
    sh:closed            true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:targetClass       aanwinst:MergedInto ;
    sh:property [
        rdfs:label "Voorlopige naam archief"@nl, "Provisional name"@en ;
        rdfs:comment "Korte weergave welk archief het is, of waar het vermoedelijk bij hoort"@nl ; 
        sh:maxCount 1 ;
        sh:path rico:hasOrHadIdentifier ;
        sh:order 1.0 ;
        sh:datatype xsd:string ;
    ] ;
    sh:property [
        rdfs:label "Aanvulling op archief"@nl, "Addition to fonds"@en ;
        rdfs:comment "Kies Ja als er al eerder bijbehorend archief is geregistreerd"@nl ; 
        sh:maxCount 1 ;
        sh:path rico:accrual ;
        sh:order 2.0 ;
        sh:datatype xsd:boolean ;
    ] ;
    sh:property [
        rdfs:label   "Archief"@nl, "Fonds"@en ;
        rdfs:comment "Kies hier het archief waar de aanwinst bij hoort"@nl ; 
        sh:order     3.0 ;
        sh:maxCount 1 ;
        sh:path      rico:isOrWasPartOf ;
        sh:nodeKind sh:IRI ;
        sh:or (
            [ sh:class recordtypes:Archiefblok ]
        ) ;
        dash:editor memorix:LinkedRecordEditor ;
    ] ;
    sh:property [
        rdfs:label   "Gerelateerde toegang"@nl, "Related fonds"@en ;
        sh:order     4.0 ;
        sh:maxCount 1 ;
        sh:path      rico:isRelatedTo ;
        sh:nodeKind sh:IRI ;
        sh:or (
            [ sh:class recordtypes:Archiefblok ]
        ) ;
        dash:editor memorix:LinkedRecordEditor ;
    ] ;
.
