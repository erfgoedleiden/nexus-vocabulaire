    sh:nodeKind sh:IRI ;
  ] ;
  sh:property [
    rdfs:label "Annotatie"@nl ;
    sh:path bibliotheek:annotation ;
    sh:maxCount 1 ;
    sh:order 14.0 ;
    sh:group bibliotheek:physicalDescriptionGroup ;
    sh:datatype xsd:string ;
